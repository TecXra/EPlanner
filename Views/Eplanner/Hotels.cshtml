                                    <datalist id="adults">
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                        <option>4</option>
                                        <option>5</option>
                                        <option>6 & up</option>
                                    </datalist>
                                    <input list="children" name="children" class="form-control" placeholder="--Children--" style="width:240px; border-radius:5px;">
                                    <datalist id="children">
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                        <option>4</option>
                                        <option>5</option>
                                        <option>6 & up</option>
                                    </datalist>
                                </div>
                            </div>
                        </div>
