                                    <label>Room Type: </label>
                                    <input list="room" name="room" class="form-control" placeholder="--Select Room Type--" style="width:100%;; border-radius:5px;">
                                    <datalist id="room">
                                        <option>Single Room</option>
                                        <option>Double Room</option>
                                        <option>Adjoing Room</option>
                                        <option>Studio Room</option>
                                        <option>Deluxe Room</option>
                                        <option>Club Room</option>
                                        <option>Executive Suite Room</option>
                                    </datalist>
                                </div>
                            </div>
                            <div class="form-group col-lg-6">
