<html>
<head>
    <title>Contact Us Us</title>
</head>
<body style="background-color: rgb(80,200,170);">
    <!--Body Starts Here-->
    <div class="container-fluid">
        <div class="row">
            <!--Slider Starts-->
            <div id="jssor_1" style="position: relative; margin: 0 auto; width: 1300px; height: 450px; overflow: hidden; visibility: hidden;">
                <!-- Loading Screen -->
                <div data-u="loading" style="position: absolute; top: 0px; left: 0px;">
                    <div style="filter: alpha(opacity=70); opacity: 0.7; position: absolute; display: block; width: 100%; height: 100%;"></div>
                    <div style="position:absolute;display:block;background:url('../../Slider/img/loading.gif') no-repeat center center;top:0px;left:0px;width:100%;height:100%;"></div>
                </div>
                <div data-u="slides" style="cursor: default; position: relative;width: 1300px; height: 500px; overflow: hidden;">
                    <div data-p="225.00" style="display: none;">
                        <img data-u="image" src="~/Slider/img/New-folder/2.jpg" />
                    </div>
                    <div data-p="225.00" style="display: none;">
                        <img data-u="image" src="~/Slider/img/New-folder/1.jpg" />
                    </div>
                    <div data-p="225.00" style="display: none;">
                        <img data-u="image" src="~/Slider/img/New-folder/3.jpg" />
                    </div>
                    <div data-p="225.00" style="display: none;">
                        <img data-u="image" src="~/Slider/img/New-folder/4.jpg" />
                    </div>
                    <div data-p="225.00" style="display: none;">
                        <img data-u="image" src="~/Slider/img/New-folder/5.jpg" />
                    </div>
                </div>
                <!-- Bullet Navigator -->
                <div data-u="navigator" class="jssorb05" style="bottom:40px; right:16px;" data-autocenter="1">
                    <!-- bullet navigator item prototype -->
                    <div data-u="prototype" style="width:25px;height:25px;">
                        <img src="~/Slider/img/b05.png">
                    </div>
                </div>
                <!-- Arrow Navigator -->
                <span data-u="arrowleft" class="jssora22L" style="left:12px;width:50px;height:70px;" data-autocenter="2">
                    <img src="~/Slider/img/a22.png" style="margin-left:-15px; margin-top:-30px;">
                </span>
                <span data-u="arrowright" class="jssora22R" style="right:12px;width:50px;height:70px;" data-autocenter="2">
                    <img src="~/Slider/img/a22.png" style="margin-left:-70px; margin-top:-30px;">
                </span>
            </div>
            <!--Slider Ends-->
            <div id="slider-search">
                <form action="~/Eplanner/Hotels">
                    <div class="col-lg-12 form-inline">
                        <div class="input-group" style="width:31%; margin-left:1%;" id="slider-input">
                            <div class="input-group-addon">Check In</div>
                            <input type="date" class="form-control" placeholder="">
                            <div class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </div>
                        </div>
                        <div class="input-group" style="width:31%; margin-left:1%;" id="slider-input">
                            <div class="input-group-addon">Check Out</div>
                            <input type="date" class="form-control" placeholder="">
                            <div class="input-group-addon">
                                <span class="glyphicon glyphicon-calendar"></span>
                            </div>
                        </div>
                        <div class="input-group" id="slider-input" style="width:30%; margin-left:1%; border-radius:5px;">
                            <input list="city" name="city" class="form-control" placeholder="--Select City--" style="">
                            <datalist id="city">
                                <option>Lahore</option>
                                <option>Islamabad</option>
                                <option>Karachi</option>
                                <option>Multan</option>
                                <option>faisalbad</option>
                                <option>Peshawar</option>
                                <option>Azad Kashmir</option>
                                <option>Sawat</option>
                                <option>Quetta</option>
                                <option>Shaikhupura</option>
                                <option>Gujrat</option>
                                <option>Gujrawnwala</option>
                                <option>Rawalpindi</option>
                                <option>Hyderabad</option>
                                <option>Sialkot</option>
                                <option>Sargodha</option>
                                <option>Gilgit</option>
                                <option>Chitrl</option>
                                <option>Sukkar</option>
                                <option>Jhelum</option>
                                <option>Sahiwal</option>
                                <option>Jhang</option>
                            </datalist>
                        </div>
                        <input type="submit" value="Check Availability" class="tm-yellow-btn" style="margin-left:42%; width:180px; margin-top:1%; border-radius:5px;">
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="tm-section-header section-margin-top">
                <div class="col-lg-4 col-md-4 col-sm-4"><hr style="margin-top:9%; margin-left:5%;"></div>
                <div class="col-lg-4 col-md-4 col-sm-4"><h2 class="tm-section-title" style="font-family:cursive;">Contact Us</h2></div>
                <div class="col-lg-4 col-md-4 col-sm-4"><hr style="margin-top:9%; margin-left:3%;"></div>
            </div>
        </div>
        <div class="row" style="margin-top:-4%;">
            <!-- contact form -->
            <form action="#" method="post" class="tm-contact-form">
                <div class="col-lg-3 col-md-3">
                </div>
                <div class="col-lg-6 col-md-6 tm-contact-form-input">
                    <div class="form-group">
                        <input type="text" id="contact_name" class="form-control" placeholder="NAME" />
                    </div>
                    <div class="form-group">
                        <input type="email" id="contact_email" class="form-control" placeholder="EMAIL" />
                    </div>
                    <div class="form-group">
                        <input type="text" id="contact_subject" class="form-control" placeholder="SUBJECT" />
                    </div>
                    <div class="form-group">
                        <textarea id="contact_message" class="form-control" rows="6" placeholder="MESSAGE"></textarea>
                    </div>
                    <div class="form-group">
                        <button class="tm-submit-btn" type="submit" name="submit">Submit now</button>
                    </div>
                </div>
                <div class="col-lg-3 col-md-3">
                </div>
            </form>
        </div>
    </div>
    <!--Body Ends Here-->

    <script>
        jssor_1_slider_init();
    </script>

</body>
</html>
























